<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:styles="clr-namespace:Material.Styles;assembly=Material.Styles"
             mc:Ignorable="d" d:DesignWidth="1500" d:DesignHeight="900"
             Background="{DynamicResource PrimaryBackgroundBrush}"
             Foreground="{DynamicResource PrimaryFontColorBrush}"
             x:Class="OpenLyricsClient.UI.View.Pages.FullLyricsPage">
    
    <Grid PointerPressed="InputElement_OnPointerPressed">
        
        <TextBlock 
            Text="Full Text"
            FontWeight="Bold"
            FontSize="18"
            HorizontalAlignment="Left"
            Margin="12, 12, 0, 0"></TextBlock>
        
        
    </Grid>
</UserControl>